---
type Header = {
  attributes: {
    logo: {
      data: {
        attributes: {
          url: string;
          alternativeText: string;
        }
      }
    },
    menu: {
      data: {
        attributes: {
          name: string;
          url: string;
        }
      }[]
    }
  };
};

const response = await fetch(`${import.meta.env.API_URL}/entete?populate=*`);
const header = await response.json();
const headerData: Header = header.data;
---

<header>
</header>
